{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "name": "Organization"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "owner": {
      "via": "organization",
      "plugin": "users-permissions",
      "model": "user"
    },
    "UID": {
      "type": "uid"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "richtext"
    }
  }
}
